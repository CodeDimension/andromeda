{
  "name": "andromeda",
  "version": "0.0.0-alpha.10",
  "main": "dist/andromeda.common.js",
  "author": {
    "email": "martinshenrique8@gmail.com",
    "name": "Henrique CustÃ³dia"
  },
  "scripts": {
    "build": "npm run lint && vue-cli-service build --target lib --name andromeda src/install.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "quotes": [
        "error",
        "single"
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "vue",
    "components",
    "library",
    "ui",
    "andromeda"
  ],
  "repository": {
    "url": "https://github.com/code-dimension/andromeda"
  }
}
